							<div class="block span-22 prepend-1 append-1 list coiffure-list" id="nos-pains-chauds">
								{% for rubrique in boucleRubriques({ "idRubrique" : 14 }) %}
									<h2 class="title">{{ rubrique.getTitre() }}</h2>

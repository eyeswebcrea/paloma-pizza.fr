								{% for rubrique in boucleRubriques({ "idRubrique" : 13 }) %}
									<h2 class="title">{{ rubrique.getTitre() }}</h2>
									<div class="content">{{ rubrique.getTexte() | raw }}</div>
								{% endfor %}
								{% for article in boucleArticles({"idRubrique" : 13}) %}
									<div class="block {{ loop.index % 2 == 0 ? 'paire' : 'impaire' }} item coiffure-item" id="nos-sandwiches-froids-item-{{ loop.index }}">
